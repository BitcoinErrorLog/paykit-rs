<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude encrypted preferences from cloud backup -->
        <exclude domain="sharedpref" path="paykit_demo_storage.xml"/>
        <exclude domain="sharedpref" path="paykit_secure_storage.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude from device-to-device transfer -->
        <exclude domain="sharedpref" path="paykit_demo_storage.xml"/>
        <exclude domain="sharedpref" path="paykit_secure_storage.xml"/>
    </device-transfer>
</data-extraction-rules>
